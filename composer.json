{
    "name": "makeabledk/laravel-modules",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelModules\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rasmus Christoffer Nielsen"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/console": "^5.8|^6.0|^7.0",
        "illuminate/container": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "require-dev": {
        "laravel/laravel": "^6.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "codedungeon/phpunit-result-printer": "^0.26.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelModules\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelModules\\ModulesServiceProvider"
            ]
        }
    }
}
